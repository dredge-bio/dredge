{
  "compilerOptions": {
    "lib": [
      "es2019",
      "dom"
    ],

    "target": "ES6",

    "module": "commonjs",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,

    "strict": true,

    "downlevelIteration": true,

    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
